{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "**/ThridParty/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "",
    "template": [
      "default",
      "template"
    ],
    "globalMetadata": {
      "_appName": "AngeliA",
      "_appTitle": "AngeliA",
      "_enableSearch": true,
      "pdf": true,
      "_appFooter": "🐄🐦‍⬛🧦🐈🎃💓🌹☕🏴‍☠️🐤",
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true
    }
  }
}